{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-ide-helper": "^2.5",
        "cviebrock/eloquent-sluggable": "4.5",
        "davejamesmiller/laravel-breadcrumbs": "^5.1",
        "doctrine/dbal": "^2.8",
        "dompdf/dompdf": "^0.8.2",
        "fideloper/proxy": "~4.0",
        "gloudemans/shoppingcart": "^2.5",
        "hekmatinasser/verta": "^1.9",
        "landish/pagination": "^1.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "mpdf/mpdf": "^7.1",
        "pusher/pusher-php-server": "^3.2",
        "rap2hpoutre/laravel-log-viewer": "^0.22.1",
        "simplesoftwareio/simple-qrcode": "~2",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.27",
        "tightenco/ziggy": "^0.6.9",
        "torann/geoip": "^1.0",
        "yajra/laravel-datatables-oracle": "^8.11",
        "zarinpal/zarinpal": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
